<!ELEMENT dc.format.extent (#PCDATA)>
<!ELEMENT dc.format.medium (#PCDATA)>
<!ELEMENT dc.identifier (#PCDATA)>
<!ELEMENT dc.source (#PCDATA)>
<!ELEMENT dc.language (#PCDATA)>
